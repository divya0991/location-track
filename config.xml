<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->
<icon src="tracking.png" platform="android" qualifier="ldpi" />
<icon src="tracking.png" platform="android" qualifier="mdpi" />
<icon src="tracking.png" platform="android" qualifier="hdpi" />
<icon src="tracking.png" platform="android" qualifier="xhdpi" />
<icon src="tracking.png" platform="android" qualifier="xxhdpi" />
<icon src="tracking.png" platform="android" qualifier="xxxhdpi" />
<icon src="tracking.png" platform="android" qualifier="fr-xxhdpi" />
  <name>Location Track</name>

  <description>
      An example for phonegap build docs.
  </description>

  <author href="https://build.phonegap.com" email="support@phonegap.com">
      Hardeep Shoker
  </author>
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
<uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
</widget>